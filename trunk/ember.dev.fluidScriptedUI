/* Ember 8.0 alpha. Begun 27 March 2014. */

MainLoadingScreen = new LoadingScreen(0);
MainLoadingScreen.show();

var set = new Object();
set["background"] = "url(\"/d/4278145217_f6f7e5f871_o.jpg\") rgba(0,0,0,0) center center no-repeat";
this.Ember = new FluidBox(set);
set=null;

var set = new Object();
set["container"] = this.Ember.id;
set["background"] = "url('http://upload.wikimedia.org/wikipedia/commons/9/9c/Image-Porkeri_001.jpg')";
set["width"] = 50;
set["heighth"] = 50;
set["vpos"] = 25;
set["hpos"] = 25;
this.bgBox = new FluidBox(set);
set=null;

var set = new Object();
set["container"] = this.Ember.id;
set["blur"] = "10";
set["width"] = 50;
set["heighth"] = 50;
set["vpos"] = 25;
set["hpos"] = 25;
this.blurBox = new FluidBox(set);
set=null;

$(window).load(function() {
	bgBox.show("none");
	blurBox.show("none");
	Ember.show("zoomhalffade");
});

/* Load user data */
$.getScript("/d/r/active.phpanother_script.js");

